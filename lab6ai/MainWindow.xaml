<Window x:Class="lab6ai.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab6ai"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="850">
    <Grid>
        <Canvas x:Name="scene" HorizontalAlignment="Left" Height="400" Margin="20,20,0,0" VerticalAlignment="Top" Width="600"/>
        <Label x:Name="LB_iteration" Content="Iteration:" HorizontalAlignment="Left" Height="40" Margin="640,20,0,0" VerticalAlignment="Top" Width="150"/>
        <Label Content="Best position:" HorizontalAlignment="Left" Height="40" Margin="640,60,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="LB_best" Content="" HorizontalAlignment="Left" Height="40" Margin="640,100,0,0" VerticalAlignment="Top" Width="150"/>
        <Button x:Name="Start_B" Content="Start" HorizontalAlignment="Left" Height="40" Margin="640,380,0,0" VerticalAlignment="Top" Width="150" Click="Start_B_Click"/>

    </Grid>
</Window>
